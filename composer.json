{
  "name": "daylerees/kurenai",
  "description": "Multi document parser with metadata support.",
  "keywords": ["kurenai", "markdown", "metadata", "document", "daylerees"],
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Kurenai\\": "src"
    }
  },
  "require-dev": {
    "phpunit/phpunit": ">=4.8",
    "squizlabs/php_codesniffer": "^2.5",
    "jakub-onderka/php-parallel-lint": "^0.9.2",
    "phing/phing": "^2.13",
    "michelf/php-markdown": "^1.6",
    "symfony/yaml": "^3.0",
    "erusev/parsedown": "^1.6",
    "netcarver/textile": "^3.5"
  },
  "require": {
    "illuminate/support": "^5.2"
  },
  "suggest": {
    "michelf/php-markdown": "Allow markdown and markdown extra content parsing.",
    "symfony/yaml": "Allow for YAML metadata parsing.",
    "erusev/parsedown": "Allow for Parsedown markdown parsing.",
    "netcarver/textile": "Allow for Textile parsing."
  }
}
